PROJECT(UDP)
CMAKE_MINIMUM_REQUIRED(VERSION 2.6) 
set(CMAKE_CXX_FLAGS "${CMAKE_CXX_FLAGS}  -std=c++0x")
SET(LIBHELLO_SRC UDPServer.cpp)
ADD_LIBRARY(UDPServer SHARED ${LIBHELLO_SRC})
SET_TARGET_PROPERTIES(UDPServer PROPERTIES CLEAN_DIRECT_OUTPUT 1)

ADD_LIBRARY(UDPServer_static STATIC ${LIBHELLO_SRC})
SET_TARGET_PROPERTIES(UDPServer_static PROPERTIES CLEAN_DIRECT_OUTPUT 1)
SET_TARGET_PROPERTIES(UDPServer_static PROPERTIES OUTPUT_NAME "UDPServer")

